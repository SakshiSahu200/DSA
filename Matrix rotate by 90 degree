int temp[n][n];
         for(int i=0;i<n;i++)
           {
               for(int j=0;j<n;j++)
               {
                   temp[n-j-1][i]= matrix[i][j];
               }
           }
           for(int i=0;i<n;i++)
           {
               for(int j=0;j<n;j++)
               {
                    matrix[i][j]= temp[i][j];
               }
           }
